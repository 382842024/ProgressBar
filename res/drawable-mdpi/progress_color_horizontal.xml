<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="9dip" />
			<gradient android:startColor="#fffcb9"
				android:centerColor="#fffcb9" android:centerY="0.75"
				android:endColor="#fffcb9" android:angle="270" />
			<stroke
                android:width="1dp"
                android:color="#773f03" />
		</shape>
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="9dip" />
				<gradient android:startColor="#80C07AB8"
					android:centerColor="#f6b912" android:centerY="0.75"
					android:endColor="#f6b912" android:angle="270" />
				<stroke
                android:width="1dp"
                android:color="#773f03" />
			</shape>
		</clip>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<corners android:radius="9dip" />
				<gradient android:startColor="#f6b912"
					android:centerColor="#f6b912" android:centerY="0.75"
					android:endColor="#f6b912" android:angle="270" />
				<stroke
                android:width="1dp"
                android:color="#773f03" />
			</shape>
		</clip>
	</item>

</layer-list>